{
  "name": "tsboilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for typescript project",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "test": "jest"
  },
  "author": "Shane Arthur",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^16.0.3",
    "awesome-typescript-loader": "^3.0.0-beta.14",
    "jest": "^18.1.0",
    "ts-jest": "^18.0.1",
    "ts-loader": "^1.3.3",
    "typescript": "^2.1.4",
    "webpack": "^1.14.0"
  },
  "dependencies": {},
  "jest": {
 	"moduleFileExtensions": [
 		"ts",
 		"js"
 	],
 	"transform": {
 		"\\.(ts)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
 	},
 	 "testRegex": "src/.*\\.spec.(ts|js)$",
 	"testResultsProcessor": "<rootDir>/node_modules/ts-jest/coverageprocessor.js",
 	"globals": {
 		"__TS_CONFIG__": {
 			"target": "es6",
 			"module": "commonjs",
 			"moduleResolution": "node"
 		}
 	}
 }
}